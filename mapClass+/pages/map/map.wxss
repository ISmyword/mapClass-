
.section{
  width: 100%;
  height: 80rpx;
}
.section input{
  width:90%;
  margin:10rpx auto;
  border:1px solid #c3c3c3;
  height:60rpx;
  border-radius: 5rpx;
  padding: 0 10rpx;
}
.map-tab-ul{
  margin-top: 10rpx;
}
.map-tab-li{
  float: left;
  width: 180rpx;
  height: 50rpx;
  line-height: 50rpx;
  border: 1rpx solid #c3c3c3;
  margin: 10rpx 33rpx;
  text-align: center;
  font-size: 25rpx;
}
.active{
  color: red;
  border-color: red;
}
.mapview{
  width: 100%;
  height: 700rpx;
}
.navigate-button{
  margin-top: 30rpx;
}
.navigate-info{
  position: fixed;
  width: 100%;
  bottom: 0;
  border-radius: 40rpx;
  background-color: #ffffff;
  z-index: 2;
}
.navigate-info-li{
  width: 710rpx;
  height: 125rpx;
  margin:  0rpx 0rpx;
  padding:  20rpx 20rpx 25rpx;
  background-color: #ffffff;
  border-bottom: 2rpx solid #eeeeee;
}
.first{
  border-bottom: 5rpx solid #eeeeee;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  margin-top: 20rpx;
}
.navigate-info-li .left{
  float: left;
  width: 480rpx;
  padding-right: 20rpx;
  height: 100%;

}
.navigate-info-li .right{
  float: left;
  width: 210rpx;
  height: 100%;
}
.navigate-info-li .right image{
  width: 20rpx;
  height: 20rpx;
}
.title{
 height: 50rpx;
 line-height: 50rpx;
 font-size: 30rpx;
 font-weight: 600;
 text-overflow: ellipsis;
 overflow: hidden;
 white-space: nowrap;
}
.address,.phone{
 height: 40rpx;
 line-height: 40rpx;
 font-size: 25rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.navigate-info-li .right .btn{
  float: left;
  width: 50%;
  height: 130rpx;
  line-height: 130rpx;
}
.phone-tbn{
  color: #ff8e8e;
}
.phone-tbn text{
  color: #bdbdbd;
}
.navigate-btn{
  color: #008aff;
}
.nomore{
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
}
.nomore{
  width: 710rpx;
  height: 125rpx;
  margin:  0rpx 0rpx;
  padding:  15rpx 20rpx 20rpx;
  background-color: #ffffff;
  /* border-bottom: 5rpx solid #eeeeee; */
   border-radius: 20rpx;
  margin-top: 20rpx;
}
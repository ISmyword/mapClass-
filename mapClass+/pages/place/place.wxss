.map-view{
  width: 100%;
  height: 700rpx;
}
.class-panel{
  position: fixed;
  width: 100%;
  bottom: 80rpx;
  border-radius: 40rpx;
  background: #ffffff;
  z-index: 2;
}
.place-title{
  width: 700rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 40rpx;
  margin-left: 50rpx;
  font-size: 35rpx;
  font-weight: 600;

}
.place-info{
  width: 700rpx;
  line-height: 60rpx;
  margin-left: 50rpx;
  font-size: 25rpx;

}
.palce-btn{
  position: fixed;
  width: 710rpx;
  margin: 0 20rpx;
  bottom: 5rpx;
  z-index: 3;
}
.palce-btn>view{
  float: left;
  width: 350rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background-color: #f8f8f8;
  border-radius: 4px;
  font-weight: 700;
  font-size: 30rpx;
}

.navigate{
  color: #ffffff;
  margin-left: 10rpx;
  background-color: #07c160 !important;
}

.class{
  width: 710rpx;
  height: 145rpx;
  margin:  0rpx 0rpx;
  padding:  15rpx 20rpx 20rpx;
  background-color: #ffffff;
  border-bottom: 5rpx solid #eeeeee;
  border-radius: 20rpx;
  margin-top: 20rpx;
}

.title{
  padding: 10rpx 0;
  font-size: 28rpx !important;
  font-weight: 600;

}
.class-info-top{
  float: left;
  width: 100%;
  height: 43rpx;
  line-height: 43rpx;
  font-size: 25rpx;
}
.class-li{
  width: 710rpx;
  height: 43rpx;
  line-height: 43rpx;
  padding:  15rpx 20rpx 20rpx;
  font-size: 25rpx;
  white-space:pre-wrap;
}

.label{
  display: block;
  float: left;
  width: 112rpx;
  height: 43rpx;
}


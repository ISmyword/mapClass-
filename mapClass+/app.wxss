/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  justify-content: space-between;
  padding: 0 0;
  box-sizing: border-box;
  font-size: 28rpx;

} 
.head{
  position: relative;
  width: 100%;
  height: 85rpx;
  background-color: #ff6c8a;
}
.head-back{
  position: absolute;
  width: 35rpx;
  height: 35rpx;
  margin: 25rpx 25rpx;
  z-index: 2;

}
.head-title {
  float: left;
  width: 670rpx;
  height: 85rpx;
  line-height: 85rpx;
  margin-left: 80rpx;
  text-align: left;
  color: #ffffff;
  font-size: 35rpx;
}
.fl{
  float: left;
}
.oneLine{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}